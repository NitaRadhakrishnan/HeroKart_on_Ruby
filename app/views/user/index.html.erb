<h1>All Users</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Date of Birth</th>
      <th>Phone Number</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <td><%= user.id%></td>
    <td><%= user.name %></td>
    <td><%= user.date_of_birth %></td>
    <td><%= user.phone_number %></td>
    <td><%= user.address %></td>
    <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>   
  </tbody>
  <% end%>
</table>

<%= link_to 'Back', admin_show_path %>